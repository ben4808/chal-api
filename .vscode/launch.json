 {
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch with ts-node",
      "program": "${workspaceFolder}/src/index.ts",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "runtimeExecutable": "ts-node",
      "env": {
        "NODE_ENV": "development"
      },
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}